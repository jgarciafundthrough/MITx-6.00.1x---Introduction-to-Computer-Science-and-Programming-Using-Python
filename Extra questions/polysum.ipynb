   "source": [
    "#Function = area + perimiter^(2)\n",
    "#Rounded to 4 digits\n",
    "\n",
    "import math\n",
    "\n",
    "def polysum(n, s):\n",
    "    area = (0.25 * n * s**2) / (math.tan(math.pi/n))\n",
    "    perim = n * s\n",
    "    result = area + (perim**2)\n",
    "\n",
    "    return(round(result, 4))"
   ]
